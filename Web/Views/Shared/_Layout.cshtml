<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"]</title>

   
        <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.css" />
    
  
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/4.1.3/css/bootstrap.min.css" crossorigin="anonymous" integrity="sha256-eSi1q2PG6J7g7ib17yAaWMcrr5GrtohYChqibrV7PBE=" />
    <link rel="stylesheet" href="~/css/site.css" />
    <script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.7.3/Chart.bundle.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.3.1/jquery.min.js" crossorigin="anonymous" integrity="sha256-FgpCb/KJQlLNfOu91ta32o/NMZxltwRo8QtmkMRdAu8="></script>
    
</head>
<body>


   
    <div class="d-flex" id="wrapper">
        
        <!-- Sidebar -->
        <div class="bg-light border-right" id="sidebar-wrapper">
            <div class="sidebar-heading">Stats</div>
            <div class="list-group list-group-flush">

                <a class="list-group-item list-group-item-action bg-light" asp-area="" asp-controller="ExchangeRate" asp-action="BankTickets">Bank tickets</a>
                <a class="list-group-item list-group-item-action bg-light" asp-area="" asp-controller="ExchangeRate" asp-action="Recommendations">Recommendations</a>
                <a class="list-group-item list-group-item-action bg-light" asp-area="" asp-controller="ExchangeRate" asp-action="Wholesales">Wholesales</a>
                <a class="list-group-item list-group-item-action bg-light" asp-area="" asp-controller="ExchangeRate" asp-action="BestTickets">Best tickets</a>
                <a class="list-group-item list-group-item-action bg-light" asp-area="" asp-controller="ExchangeRate" asp-action="CurrencyPrice">Currency price</a>
                <a class="list-group-item list-group-item-action bg-light" asp-area="" asp-controller="ExchangeRate" asp-action="CurrencyTimeline">Currency timeline</a>
                <a class="list-group-item list-group-item-action bg-light" asp-area="" asp-controller="ExchangeRate" asp-action="MixTickets"><strong>Mix</strong></a>
            </div>
        </div>
        <!-- /#sidebar-wrapper -->
        <!-- Page Content -->


        <div id="page-content-wrapper">
            <header>
                <nav class="navbar navbar-expand-lg navbar-light bg-light">
                    <div class="btn btn-outline-dark" id="menu-toggle">Stats sidebar</div>

                    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNavAltMarkup" aria-controls="navbarNavAltMarkup" aria-expanded="false" aria-label="Toggle navigation">
                        <span class="navbar-toggler-icon"></span>
                    </button>

                    <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
                        <div class="navbar-nav">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                            <a class="nav-link text-dark" asp-area="" asp-controller="ExchangeRate" asp-action="Index">Introduction</a>
                            <a class="nav-link text-dark" asp-area="" asp-controller="Utillities" asp-action="Index">Utillity</a>

                        </div>
                    </div>
                </nav>
            </header>





            <div class="container-fluid p-4">
                <!-- /#page-content-wrapper -->
               
                @RenderBody()
            </div>
        </div>
    </div>
    <footer class="footer  bg-white border-top border-info" >
        <div class="container">
            <div class="row">
                <div class="col justify-content-center d-flex">
                    <span class="text-muted"><strong>Contact: </strong>Michael Melena | michael.melena@tul.cz</span>
                </div>
                <div class="col justify-content-center d-flex">
                    <span class="text-muted"><strong>Version:</strong> @(ViewBag.version != null? ViewBag.version.ToString():"Unavailable")</span>
                </div>
            </div>
            
        </div>
            
                    
                    
                
            
        
    </footer>







    <script src="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/4.1.3/js/bootstrap.bundle.min.js"
            crossorigin="anonymous"
            integrity="sha256-E/V4cWE4qvAeO5MOhjtGtqDzPndRO1LBk8lJ/PR7CA4=">
    </script>
    <script src="~/js/site.js"></script>
    <script>
        $("#menu-toggle").click(function (e) {
            e.preventDefault();
            $("#wrapper").toggleClass("toggled");
        });
    </script>
    @RenderSection("Scripts", required: false)

</body>
    </html>
